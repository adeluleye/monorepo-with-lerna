{
  "name": "lerna-monorepo-starter",
  "description": "Lerna monorepo starter",
  "version": "1.0.0",
  "author": "Andrico Karoulla <karoulla.andrico@gmail.com>",
  "dependencies": {
    "lerna": "^3.21.0",
    "react": "^16.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "run:blog": "cd packages/blog && yarn develop",
    "run:shop": "cd packages/shop && yarn develop",
    "run:all": "npm-run-all --parallel run:blog run:shop",
    "test": "jest test --config ./jest-config.json"
  },
  "engines": {
    "node": "12.16.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.2",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "react-router-dom": "^4.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ]
}
